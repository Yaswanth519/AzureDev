			"apiVersion": "2018-06-01",
			"properties": {
				"description": "This is Linked Service for copy csv",
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AZ_CSV_LS_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('AZ_CSV_LS_accountKey')]"
					}
				}
			},
			"dependsOn": []
